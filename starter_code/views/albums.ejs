<h1>Albums</h1>
<ul>
    <% albums.forEach( album => { %>
        <li>
            <h2><%= album.name %></h2>
            <a class="btn btn-default" href="/tracks/<%=album.id%>">View Tracks</a>
            <% if (album.images.length > 0) { %>
            <img src="<%= album.images[0].url %>" alt="">
            <% } %>
        </li>

    <% }); %>
</ul>