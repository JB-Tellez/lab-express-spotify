<h1>Artists</h1>
<ul>
    <% artists.forEach( artist => { %>
        <li>
            <h2><%= artist.name %></h2>
            <a class="btn btn-default" href="/albums/<%=artist.id%>">View Albums</a>
            <% if (artist.images.length > 0) { %>
            <img src="<%= artist.images[0].url %>" alt="">
            <% } %>
        </li>

    <% }); %>
</ul>